source 'https://github.com/tianbinbin/TBMtimePodSpec.git'        # 自定义索引库
source 'https://github.com/CocoaPods/Specs.git'                  # cocopod 索引库
   

# 此处是管理cocoapod第三方库的显示状态 加上这两句之后 看起来更有层次了
install! 'cocoapods', :generate_multiple_pod_projects => true

platform :ios, '11.0'

target 'TBMtimeProject' do

  pod 'TBMtimeCommon', '0.0.1'
   

end
